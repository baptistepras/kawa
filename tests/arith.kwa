
var int a;
var int c;

main {
  a = 42;
  c = 42;
  /* Affiche 42 */
  print(40 - 2 - (3 + 1) * 4 / 8 + 6);  

  /* Binop -, affiche 35 */
  print(40-5);

  /* Binop +, affiche 45 */
  print(40+5); 

  /* Binop *, affiche 200 */
  print(40*5); 

  /* Binop /, affiche 8 */
  print(40/5); 

  /* Binop Rem, affiche 5 */
  print(40%7); 

  /* Unop -, affiche -5, 5 et -5 */
  print(---5);
  print(--5);
  print(-5);

  /* Binop ==, affiche True, False, True, True */
  print(true == true);
  print(42 == 43);
  print(a == a);
  print(a == c);

  /* Binop !=, affiche False, True, False, False */
  print(true != true);
  print(42 != 43);
  print(a != a);
  print(a != c);

  /* Binop &&, affiche True, False, False, False */
  print(true && true);
  print(true && false);
  print(false && true);
  print(false && false);

  /* Binop ||, affiche True, True, True, False */
  print(true || true);
  print(true || false);
  print(false || true);
  print(false || false);

  /* Unop !, False, True, True */
  print(!true);
  print(!false);
  print(!!true);
}
